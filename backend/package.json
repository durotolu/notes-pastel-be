{
  "name": "notes-pastel-be",
  "version": "1.0.0",
  "description": "Notes Pastel Backend",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "server": "nodemon src/index.ts",
    "dev": "concurrently \"npm run server\" \"npm run frontend\"",
    "frontend-install": "cd ../ && npm install --prefix frontend",
    "frontend": "cd ../ && npm run start --prefix frontend"
  },
  "dependencies": {
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.15",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
